pipeline {
    agent any
    tools {
    maven 'M2'
    }
    stages {
        stage ('Build') {
            steps {
                git 'https://github.com/manigandan1821/JavaCode.git'
                sh 'mvn clean package'
            }
        }
   }
}

	stage('SonarQube Analysis') {
	  def mvnHome = tool name: 'M2', type: 'maven'
	  withSonarQubeEnv('sonar') {
	    sh "${mvnHome}/bin/mvn sonar:sonar"
	  }
     }
